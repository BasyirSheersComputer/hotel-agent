{
    "dashboard": {
        "name": "AnalyticsDashboard",
        "resolution": "1440p",
        "background": "#FFFFFF",
        "sections": [
            {
                "id": "kpi_row",
                "type": "horizontal-cards",
                "cards": [
                    {
                        "id": "total_queries",
                        "icon": "chat-bubble",
                        "value_binding": "metrics.totalQueries",
                        "label": "Total Queries Today",
                        "style": "kpi_card_primary"
                    },
                    {
                        "id": "avg_response_time",
                        "icon": "bolt",
                        "value_binding": "metrics.avgResponseTimeMs",
                        "label": "Avg Response Time",
                        "badge_logic": {
                            "condition": "metrics.avgResponseTimeMs > 3000",
                            "text": "Needs improvement",
                            "color": "warning"
                        },
                        "style": "kpi_card_primary"
                    },
                    {
                        "id": "accuracy_score",
                        "icon": "check-badge",
                        "value_binding": "metrics.accuracyPercent",
                        "sub_values": {
                            "internal_accuracy": "metrics.internalAccuracyPercent",
                            "external_accuracy": "metrics.externalAccuracyPercent"
                        },
                        "label": "Accuracy Score",
                        "style": "kpi_card_primary"
                    },
                    {
                        "id": "aht_reduction",
                        "icon": "trending-up",
                        "value_binding": "metrics.ahtReductionPercent",
                        "sub_values": {
                            "delta": "metrics.ahtDeltaPercent"
                        },
                        "label": "AHT Reduction",
                        "style": "kpi_card_primary"
                    }
                ]
            },
            {
                "id": "query_sources",
                "type": "bar-breakdown",
                "title": "Query Sources",
                "items": [
                    {
                        "id": "rag_queries",
                        "label": "RAG Queries",
                        "value_binding": "metrics.ragPercentage",
                        "count_binding": "metrics.ragCount",
                        "bar_color": "navy"
                    },
                    {
                        "id": "maps_queries",
                        "label": "Maps Queries",
                        "value_binding": "metrics.mapsPercentage",
                        "count_binding": "metrics.mapsCount",
                        "bar_color": "teal"
                    }
                ]
            },
            {
                "id": "question_categories",
                "type": "pill-cards",
                "title": "Top Guest Question Categories",
                "pill_style": "light_outline",
                "items_binding": "metrics.questionCategories",
                "pill_schema": {
                    "fields": [
                        "category",
                        "count",
                        "avg_ai_time",
                        "accuracy"
                    ]
                }
            },
            {
                "id": "hourly_trends",
                "type": "line-chart",
                "title": "Hourly Trends",
                "data_binding": "metrics.hourlyDistribution",
                "color": "purple",
                "axis": {
                    "x": "time",
                    "y": "queryVolume"
                },
                "style": "chart_card"
            },
            {
                "id": "agent_performance",
                "type": "table",
                "title": "Agent Performance",
                "columns": [
                    {
                        "id": "agent_name",
                        "label": "Agent",
                        "binding": "name"
                    },
                    {
                        "id": "queries",
                        "label": "Queries Handled",
                        "binding": "queryCount"
                    },
                    {
                        "id": "avg_time",
                        "label": "Avg Response Time",
                        "binding": "avgTimeMs"
                    },
                    {
                        "id": "accuracy",
                        "label": "Accuracy",
                        "binding": "accuracyPercent"
                    },
                    {
                        "id": "last_active",
                        "label": "Last Active",
                        "binding": "lastActive"
                    }
                ],
                "data_binding": "agents"
            },
            {
                "id": "api_cost_usage",
                "type": "summary_card",
                "title": "API Usage & Cost",
                "fields": [
                    {
                        "label": "Tokens Used",
                        "binding": "metrics.tokensUsed"
                    },
                    {
                        "label": "Estimated Cost",
                        "binding": "metrics.estimatedCost"
                    },
                    {
                        "label": "Rate Limit Status",
                        "binding": "metrics.rateLimitStatus"
                    },
                    {
                        "label": "Breakdown",
                        "binding": "metrics.costBreakdown"
                    }
                ],
                "style": "side_card"
            }
        ],
        "styles": {
            "kpi_card_primary": {
                "background": "#FFFFFF",
                "border": "1px solid #E1E4EA",
                "radius": 12,
                "padding": 20,
                "font": "Inter-600"
            },
            "chart_card": {
                "background": "#FFFFFF",
                "border": "1px solid #E1E4EA",
                "radius": 12
            },
            "side_card": {
                "background": "#FFFFFF",
                "border": "1px solid #E1E4EA",
                "radius": 12,
                "padding": 20,
                "font": "Inter-500"
            }
        }
    }
}